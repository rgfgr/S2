<Window x:Class="Netto_App.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Netto_App"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" Loaded="Window_Loaded">
    <Window.Resources>
        <local:Netto_dbDataSet x:Key="netto_dbDataSet"/>
        <CollectionViewSource x:Key="varreViewSource" Source="{Binding Varre, Source={StaticResource netto_dbDataSet}}"/>
    </Window.Resources>
    <Grid DataContext="{StaticResource varreViewSource}">
        <DataGrid Height="337" Margin="41,31,101,0" VerticalAlignment="Top" ItemsSource="{Binding}"/>

    </Grid>
</Window>
